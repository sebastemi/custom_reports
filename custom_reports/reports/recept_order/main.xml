<odoo>
    <data>
        <template id="report_recept_order_main">
            <div class="main" style="font-family:arial;">
                <table style="font-size:11px;width:100%;">
                    <thead>
                        <tr style="border:1px solid #fff;">
                            <th colspan="6" style="padding:10px;border-bottom:1px solid #000;"></th>
                            <th colspan="3" class="text-center" style="padding:10px;border:1px solid #000;border-bottom:1px solid #000;">16. Estados</th>
                            <th colspan="1" style="padding:10px;border-bottom:1px solid #000;"></th>
                        </tr>
                        <tr>
                            <th style="padding:10px;">10. Linea</th>
                            <th style="padding:10px;">11. Articulo</th>
                            <th style="padding:10px;">12. Descripcion</th>
                            <th style="padding:10px;">13. Cant. O/C</th>
                            <th style="padding:10px;">14. Cant. Recibida</th>
                            <th style="padding:10px;">15. Cant. Pendiente</th>
                            <th style="padding:10px;">A</th>
                            <th style="padding:10px;">AD</th>
                            <th style="padding:10px;">R</th>
                            <th style="padding:10px;">17. Lote</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="doc.move_ids_without_package" t-as="line">
                            <t t-foreach="line.move_line_ids" t-as="subline">
                                <tr>
                                    <td class="text-center" style="padding:10px;">
                                        <t t-esc="subline_index + 1"/>
                                    </td>
                                    <td style="padding:10px;">
                                        <t t-esc="line.product_id.default_code"/>
                                    </td>
                                    <td style="padding:10px;">
                                        <t t-esc="line.product_id.name"/>
                                    </td>
                                    <td style="text-align:right;padding:10px;">
                                        <t t-esc="'{:.2f}'.format(line.product_uom_qty)"/>
                                    </td>
                                    <td style="text-align:right;padding:10px;">
                                        <t t-esc="'{:.2f}'.format(subline.quantity)"/>
                                    </td>
                                    <td style="text-align:right;padding:10px;"> 
                                        <t t-esc="'{:.2f}'.format(line.product_uom_qty - line.quantity)"/>
                                    </td>
                                    <td style="padding:10px;"></td>
                                    <td style="padding:10px;"></td>
                                    <td style="padding:10px;"></td>
                                    <td style="padding:10px;">
                                        <t t-esc="subline.lot_id.name"/>
                                    </td>
                                </tr>
                            </t>
                        </t>
                    </tbody>
                </table>
            </div>
        </template>
    </data>
</odoo>